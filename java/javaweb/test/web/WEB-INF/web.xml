<?xml version="1.0" encoding="UTF-8"?>
<com.web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/com.web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.test.helloServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>helloServlet2</servlet-name>
        <servlet-class>com.test.helloServlet2</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
          <servlet-name>helloServlet2</servlet-name>
          <url-pattern>/hello2</url-pattern>
    </servlet-mapping>
</com.web-app>